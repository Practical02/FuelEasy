{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "functions": {
    "api/vercel.ts": {
      "runtime": "@vercel/node@5.3.10",
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api/vercel.ts"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
} 